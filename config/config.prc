# Window options...
window-title Pirates Online Remake
icon-filename pirates.ico

# Filenames/filepaths...
model-path resources/
model-path resources/phase_2
model-path resources/phase_3
model-path resources/phase_4
model-path resources/phase_5

default-model-extension .bam

# Server
server-version piratesremake-1.0.0

#Resolution
win-size 800 600

# Graphics
want-improved-graphics 1
want-portal-cull 1
want-shaders 1
framebuffer-multisample 1
multisamples 2

# Performance
want-preloader 1

# Minigames
want-fishing-game 0
want-repair-game 0
want-potion-game 0
want-cannondefense-game 0

# Sailing
want-seapatch 1
ships-follow-water 1
want-water-alpha 1

#Make-A-Pirate
want-random-gender 1
want-random-dna 1

# Combat
skill-recharge-time -1.0

# Worlds
want-welcome-worlds 1

# Magic Words
want-chat 1
want-run 0
want-immortal-mode 0

# Audio
audio-music-active 1

# Animation
osd-anim-blends 0

# Features
want-special-effects 1
want-momentary-minimap 1
want-inventory 0
want-tutorial 1
want-guilds 0

want-water-debug-keys 0
want-water-parameter-ui 0
want-water-panel 0

show-attack-calc 0
instant-skill-recharge 0
show-ai-cannon-targets 0
